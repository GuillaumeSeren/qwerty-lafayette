<dead_grave> <j>                      : "↓"  U2193
<dead_grave> <k>                      : "↑"  U2191
<dead_grave> <l>                      : "→"  U2192
<dead_grave> <H>                      : "⇐"  U21d0
<dead_grave> <J>                      : "⇓"  U21d3
<dead_grave> <K>                      : "⇑"  U21d1
